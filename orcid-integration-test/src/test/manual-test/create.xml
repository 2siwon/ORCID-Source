<?xml-model href="https://raw.github.com/ORCID/ORCID-Source/master/orcid-model/src/main/resources/orcid-message-1.2_rc1.xsd" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<orcid-message xmlns="http://www.orcid.org/ns/orcid"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.orcid.org/ns/orcid https://raw.github.com/ORCID/ORCID-Source/master/orcid-model/src/main/resources/orcid-message-1.2.xsd">
    <message-version>1.2</message-version>
    <orcid-profile>
        <orcid-preferences>
            <locale>en</locale>
        </orcid-preferences>
         <orcid-bio>
             
            <personal-details>
                <given-names>Given</given-names>
                <family-name>Family</family-name>
                <credit-name>API created record</credit-name>
                <other-names>
                    <other-name>AKA</other-name>
                </other-names>
            </personal-details>
             
             <biography>Bio</biography>
             <researcher-urls>
                 <researcher-url>
                     <url-name>URL</url-name>
