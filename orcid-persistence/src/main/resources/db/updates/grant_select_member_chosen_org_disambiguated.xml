<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
	
	<changeSet id="GRANT-READ-PERMISSIONS-TO-ORCIDRO-ON-MEMBER-CHOSEN-ORGS" author="George Nash" dbms="postgresql">
		<preConditions>
			<sqlCheck expectedResult="1">SELECT 1 FROM pg_roles WHERE rolname='orcidro'</sqlCheck>
		</preConditions>
		<sql>GRANT SELECT ON member_chosen_org_disambiguated to orcidro;</sql>
	</changeSet>
	
</databaseChangeLog>